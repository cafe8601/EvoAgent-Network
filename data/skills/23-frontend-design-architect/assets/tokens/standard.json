{
  "meta": {
    "name": "standard",
    "displayName": "Standard Token Set",
    "description": "Comprehensive token set for most web applications",
    "tokenCount": 85,
    "useCase": ["web-applications", "saas", "e-commerce", "content-sites"]
  },
  "color": {
    "primitives": {
      "gray": {
        "50": "#F9FAFB",
        "100": "#F3F4F6",
        "200": "#E5E7EB",
        "300": "#D1D5DB",
        "400": "#9CA3AF",
        "500": "#6B7280",
        "600": "#4B5563",
        "700": "#374151",
        "800": "#1F2937",
        "900": "#111827",
        "950": "#030712"
      },
      "primary": {
        "50": "#EFF6FF",
        "100": "#DBEAFE",
        "200": "#BFDBFE",
        "300": "#93C5FD",
        "400": "#60A5FA",
        "500": "#3B82F6",
        "600": "#2563EB",
        "700": "#1D4ED8",
        "800": "#1E40AF",
        "900": "#1E3A8A"
      },
      "success": {
        "50": "#F0FDF4",
        "500": "#22C55E",
        "600": "#16A34A",
        "700": "#15803D"
      },
      "warning": {
        "50": "#FFFBEB",
        "500": "#F59E0B",
        "600": "#D97706",
        "700": "#B45309"
      },
      "error": {
        "50": "#FEF2F2",
        "500": "#EF4444",
        "600": "#DC2626",
        "700": "#B91C1C"
      }
    },
    "semantic": {
      "surface": {
        "default": "var(--color-gray-50)",
        "subtle": "var(--color-gray-100)",
        "muted": "var(--color-gray-200)",
        "inverse": "var(--color-gray-900)"
      },
      "text": {
        "default": "var(--color-gray-900)",
        "muted": "var(--color-gray-600)",
        "subtle": "var(--color-gray-500)",
        "inverse": "var(--color-gray-50)"
      },
      "border": {
        "default": "var(--color-gray-200)",
        "strong": "var(--color-gray-300)",
        "focus": "var(--color-primary-500)"
      },
      "interactive": {
        "default": "var(--color-primary-500)",
        "hover": "var(--color-primary-600)",
        "active": "var(--color-primary-700)"
      }
    },
    "component": {
      "button": {
        "primary": {
          "bg": "var(--color-primary-500)",
          "bgHover": "var(--color-primary-600)",
          "text": "var(--text-inverse)"
        },
        "secondary": {
          "bg": "transparent",
          "border": "var(--border-default)",
          "text": "var(--text-default)"
        }
      }
    }
  },
